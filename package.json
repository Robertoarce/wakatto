{
  "name": "expo-typescript-starter",
  "version": "0.1.0",
  "description": "Template for expo project with TS, Storybook, Reg-Suit",
  "repository": "https://github.com/Naturalclar/expo-typescript-starter.git",
  "license": "MIT",
  "author": "Jesse Katsumata <jesse.katsumata@gmail.com>",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "build": "yarn storybook:build -o public",
    "eject": "expo eject",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext '.ts, .tsx'",
    "new": "scaffdog generate",
    "reg-suit": "reg-suit run",
    "start": "expo start",
    "storybook": "start-storybook -p 9001 -c .storybook -s .storybook/assets",
    "storybook:build": "build-storybook -c .storybook -s .storybook/assets",
    "storycap:all": "rm -rf ./__screenshots__ && yarn storycap --serverCmd 'yarn storybook --ci' --viewport '414x736' --serverTimeout '200000'",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "type-check": "tsc --noEmit"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/bottom-tabs": "^7.7.3",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/stack": "^7.6.2",
    "@supabase/supabase-js": "^2.79.0",
    "@types/react-redux": "^7.1.34",
    "@types/redux-thunk": "^2.1.32",
    "expo": "~50.0.14",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-web": "^0.19.13",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "typescript": "^5.1.3"
  }
}
