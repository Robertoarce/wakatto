{
  "name": "ai-diary",
  "version": "1.1.5",
  "description": "AI-powered personal diary with knowledge graph visualization",
  "repository": {
    "type": "git",
    "url": "https://github.com/Robertoarce/wakatto.git"
  },
  "homepage": "https://www.wakatto.com",
  "license": "MIT",
  "author": "Roberto Arce",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "build": "yarn storybook:build -o public",
    "eject": "expo eject",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:build": "expo export:web",
    "web:build:webpack": "webpack-cli --config webpack.config.js --mode production",
    "web:dev": "webpack serve --config webpack.config.js --mode development",
    "web:serve": "npx serve web-build",
    "version:bump": "node scripts/bump-version.js",
    "predeploy": "npm run version:bump && expo export:web && cp CNAME web-build/ && cp web/404.html web-build/",
    "deploy": "gh-pages -d web-build",
    "lint": "eslint . --ext '.ts, .tsx'",
    "new": "scaffdog generate",
    "reg-suit": "reg-suit run",
    "start": "expo start",
    "storybook": "start-storybook -p 9001 -c .storybook -s .storybook/assets",
    "storybook:build": "build-storybook -c .storybook -s .storybook/assets",
    "storycap:all": "rm -rf ./__screenshots__ && yarn storycap --serverCmd 'yarn storybook --ci' --viewport '414x736' --serverTimeout '200000'",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "type-check": "tsc --noEmit"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo-google-fonts/poppins": "^0.4.1",
    "@expo-google-fonts/roboto-mono": "^0.4.1",
    "@expo-google-fonts/source-sans-pro": "^0.2.3",
    "@expo-google-fonts/space-mono": "^0.4.2",
    "@expo/metro-runtime": "~3.1.3",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/bottom-tabs": "^7.7.3",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/stack": "^7.6.2",
    "@react-three/drei": "^9.105.6",
    "@react-three/fiber": "^8.16.8",
    "@supabase/supabase-js": "^2.79.0",
    "@types/react-redux": "^7.1.34",
    "@types/redux-thunk": "^2.1.32",
    "expo": "~50.0.14",
    "expo-av": "~13.10.6",
    "expo-font": "^11.10.3",
    "expo-gl": "~13.6.0",
    "expo-linking": "^8.0.11",
    "expo-secure-store": "~12.8.1",
    "expo-speech": "~11.7.0",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "^0.19.13",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "supabase": "^2.70.5",
    "three": "^0.163.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/webpack-config": "^19.0.1",
    "@playwright/test": "^1.56.1",
    "@types/react": "~18.2.45",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "gh-pages": "^6.3.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "typescript": "^5.1.3",
    "util": "^0.12.5",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^4.15.2"
  }
}
